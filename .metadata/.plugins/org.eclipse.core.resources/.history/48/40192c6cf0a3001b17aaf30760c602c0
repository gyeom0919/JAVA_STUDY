package study;

public class exam05 {

	public static void main(String[] args) {

		int a= (int) (Math.random()*4)+8;
		
		switch(a) {
		
		case(8){
			System.out.println("출근합니다.");
			System.out.println("회의를 합니다.");
			System.out.println("업무를 봅니다.");
			System.out.println("외근을 나갑니다.");
		} break;
		case(9){
			System.out.println("회의를 합니다.");
			System.out.println("업무를 봅니다.");
			System.out.println("외근을 나갑니다.");
		}break;
		case(10){
			System.out.println("업무를 봅니다.");
			System.out.println("외근을 나갑니다.");
		}break;
		default{
			System.out.println("외근을 나갑니다.");
		}
		
		
		}
			
	}

}
